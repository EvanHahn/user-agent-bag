// @flow
type Entries = Iterable<[string, string | null]>;

type UserAgentBagNode = (
  {|
    type: 'product',
    product: string,
    version: string | null
  |} |
  {|
    type: 'comment',
    text: string
  |}
);

declare class UserAgentBag {
  constructor(arg: string | Entries | null | void): UserAgentBag;

  get(product: string): string | null | void;
  getAll(product: string): Array<string | null | void>;
  has(product: string): boolean;
  entries(): Entries;
  size(): number;

  toString(): string;
}

declare module.exports: typeof UserAgentBag;
